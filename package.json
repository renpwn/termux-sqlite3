{
  "name": "@renpwn/termux-sqlite3",
  "version": "1.0.2",
  "description": "High-performance SQLite3 for Termux Android with better-sqlite3-like API",
  "main": "index.js",
  "exports": {
    ".": "./index.js",
    "./debug": "./lib/debug.js",
    "./memory": "./lib/memory.js",
    "./transaction": "./transaction.js"
  },
  "keywords": [
    "sqlite",
    "sqlite3",
    "termux",
    "android",
    "database",
    "better-sqlite3",
    "mobile",
    "embedded",
    "cli"
  ],
  "author": "renpwn",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/renpwn/termux-sqlite3.git"
  },
  "bugs": {
    "url": "https://github.com/renpwn/termux-sqlite3/issues"
  },
  "homepage": "https://github.com/renpwn/termux-sqlite3#readme",
  "engines": {
    "node": ">=14.0.0",
    "termux": ">=0.118.0"
  },
  "scripts": {
    "test": "node test/basic.test.js",
    "benchmark": "node benchmark/performance.js",
    "lint": "eslint .",
    "format": "prettier --write '**/*.js'",
    "coverage": "nyc npm test",
    "security": "npm audit",
    "build:docs": "jsdoc -c jsdoc.json"
  },
  "dependencies": {},
  "devDependencies": {
    "eslint": "^8.0.0",
    "prettier": "^3.0.0",
    "jsdoc": "^4.0.0",
    "nyc": "^15.0.0"
  },
  "files": [
    "index.js",
    "engine.js",
    "statement.js",
    "cursor.js",
    "transaction.js",
    "lib/",
    "README.md",
    "LICENSE"
  ],
  "os": ["android"],
  "cpu": ["arm", "arm64", "x64"],
  "optionalDependencies": {
    "sqlite3": ">=3.40.0"
  }
}